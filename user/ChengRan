    //1.角度判断
    float TotalLength = Front + Rear + CAR_LENGTH;
    if (TotalLength == FIELD_RIVER)
        Omega = 0;
    else if(TotalLength == FIELD_LENGTH)
    {
        if(Left < FIELD_RIVER)
    }
    return 0;
}
